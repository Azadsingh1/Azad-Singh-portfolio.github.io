{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "header":[
      {
        "source":"/api.json",
        "header":[{
          "key":"Access-Control-Allow-Origin",
          "value":"*"
        }]
      }
    ]
  }
}
